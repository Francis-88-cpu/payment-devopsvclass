pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'Hello World'
            }
        }

        stage('compile') {
            steps {
                echo 'Hello World'
            }

            stage('test') {
            steps {
                echo 'Hello World'
            }

            stage('package') {
            steps {
                echo 'Hello World'
            }

            stage('docker build') {
            steps {
                echo 'Hello World'
            }
            stage('docker login') {
            steps {
                echo 'Hello World'
            }

            stage('docker push') {
            steps {
                echo 'Hello World'
            }
        }
        }
        }
        }
        }